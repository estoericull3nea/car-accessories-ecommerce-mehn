<html lang="en">
  <head>
    <!-- {{>head}} -->
    <%- include("partials/head") %>
    <link rel="stylesheet" href="css/team.css" />

  </head>
  <body>
    <%- include("partials/header") %>

    <nav aria-label="breadcrumb" class="container mt-3">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Our Team</li>
      </ol>
    </nav>

    <!-- our team -->
    <section class="our_team_section">
      <body>
        <article class="flow">
          <h1>Our Team</h1>
          <p>Hover or focus over each card to see the person's job title slide in and the colour treatment change.</p>
          <div class="team">
            <ul class="auto-grid" role="list">
              
              
              <li>
                <a href="#" target=_blank" class="profile">
                  <h2 class="profile__name">Mica</h2>
                  <p class="pos">Designer</p>
                  <img alt="Profile shot for Marc Dixon" src="http://www.avatarsinpixels..png" />
                </a>
              </li>
              <li>
                <a href="#" class="profile">
                  <h2 class="profile__name">Guian</h2>
                  <p class="pos">Researcher</p>
                  <img alt="Profile shot for Chad" src="http://www.avatarsinpixels..png" />
                </a>
              </li>
              <li>
                <a href="#" target=_blank" class="profile">
                  <h2 class="profile__name">Arabella</h2>
                  <p class="pos">Asst. Project Manager</p>
                  <img alt="Profile shot for Nicholas Castro" src="http://www.avatarsinpixels..png" />
                </a>
              </li>
              <li>
                <a href="#" target=_blank" class="profile">
                  <h2 class="profile__name">Ericson</h2>
                  <p class="pos">Project Manager</p>
                  <img alt="Profile shot for Ruby Morris" src="http://www.avatarsinpixels..png" />
                </a>
              </li>
            </ul>
          </div>
        </article>
      </body>


      

    </section>
    <!--  -->

    <%- include("partials/footer") %>
  </body>
</html>
